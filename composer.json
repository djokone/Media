{
    "name": "djokone/media",
    "description": "Media plugin for CakePHP",
    "type": "cakephp-plugin",
    "minimum-stability" : "dev",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Media\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Media\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
